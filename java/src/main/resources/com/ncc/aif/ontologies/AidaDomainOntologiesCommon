@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://raw.githubusercontent.com/NextCenturyCorporation/AIDA-Interchange-Format/master/java/src/main/resources/com/ncc/aif/ontologies/AidaDomainOntologiesCommon> rdf:type owl:Ontology ;
    rdfs:label "Common classes across AIDA domain-specific ontologies"^^xsd:string ;
    owl:versionInfo "Version 1.1"^^xsd:string .

# This file doesn't define any leaf types, but it defines common types to allow the validator
# to work across multiple domain ontologies

######################################################
# Top-level classes
######################################################

:EntityType a owl:Class ;
   rdfs:label "AIDA entity type" .

:EventType a owl:Class ;
   rdfs:label "AIDA event type" .

:EventArgumentType a owl:Class ;
   rdfs:label "AIDA event argument type" .

:RelationType a owl:Class ;
   rdfs:label "AIDA relation type" .

:RelationArgumentType a owl:Class ;
   rdfs:label "AIDA relation argument type" .

:CanHaveName a owl:Class ;
    rdfs:label "Something allowed to have a name" .

:CanHaveTextValue a owl:Class ;
    rdfs:label "Something allowed to have a text value" .

:CanHaveNumericValue a owl:Class ;
    rdfs:label "Something allowed to have a numeric value" .
