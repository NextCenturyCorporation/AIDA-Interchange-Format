######################################################
# Event types
######################################################
ldcOnt:Business.DeclareBankruptcy
        a                owl:Class ;
        rdfs:subClassOf  aidaDomainCommon:EventType ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  [ a            owl:Class ;
                                                rdfs:label   "Time" ;
                                                owl:unionOf  ( ldcOnt:Time )
                                              ] ;
                           owl:onProperty     ldcOnt:Business.DeclareBankruptcy_Time
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  [ a            owl:Class ;
                                                rdfs:label   "Place" ;
                                                owl:unionOf  ( ldcOnt:Facility ldcOnt:GeopoliticalEntity ldcOnt:Location )
                                              ] ;
                           owl:onProperty     ldcOnt:Business.DeclareBankruptcy_Place
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  [ a            owl:Class ;
                                                rdfs:label   "Organization" ;
