ldcOnt:GeneralAffiliation.APORA
        a                owl:Class ;
        rdfs:label       "General-Affiliation.Artifact-Political-Organization-Religious-Affiliation (APORA)" ;
        rdfs:subClassOf  aidaDomainCommon:RelationType ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  [ a            owl:Class ;
                                                rdfs:label   "Affiliation" ;
                                                owl:unionOf  ( ldcOnt:GeopoliticalEntity ldcOnt:Location ldcOnt:Organization ldcOnt:Person ldcOnt:Sides )
                                              ] ;
                           owl:onProperty     ldcOnt:GeneralAffiliation.APORA_Affiliation
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  [ a            owl:Class ;
                                                rdfs:label   "Affiliate" ;
                                                owl:unionOf  ( ldcOnt:Commodity ldcOnt:Facility ldcOnt:Vehicle ldcOnt:Weapon )
                                              ] ;
                           owl:onProperty     ldcOnt:GeneralAffiliation.APORA_Affiliate
                         ] .

